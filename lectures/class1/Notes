	siva.stepIntoClass()
	yuyang.sitInClass()
	sahiti.sitInClass()
	snehal.sitInClass()
	..
	siva.greetStudents()
	siva.takeAttendance()
	siva.introduceTA(divya, terry)
	divya.greetStudents()
	terry.greetStudents()
	if(newMicrophone.isNotWorking()){
		HelpDesk hd;
		Batteries b = hd.getMeBatteries();
		if(b == null){
			siva.speakLouder();
		}
		else{
			neuMicrophone.replaceBatteries(b);
		}
	}
	neuMicrophone.amplifyLecture()
